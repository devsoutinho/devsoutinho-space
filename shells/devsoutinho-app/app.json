{
  "expo": {
    "entryPoint": "index.js",
    "owner": "devsoutinho_tech",
    "slug": "devsoutinho-app",
    "name": "Devsoutinho App",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "myapp",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.mariosouto.devsoutinho",
      "supportsTablet": true,
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.mariosouto.devsoutinho",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
